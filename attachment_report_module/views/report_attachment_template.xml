<odoo>
  <template id="report_attachment_template">
    <t t-call="web.html_container">
      <t t-foreach="docs" t-as="doc">
        <div class="page">
          <h2>Attachment: <t t-esc="doc.name"/></h2>
          <p><strong>Model:</strong> <t t-esc="doc.res_model"/></p>
          <p><strong>Record ID:</strong> <t t-esc="doc.res_id"/></p>
          <p><strong>URL:</strong> <t t-esc="doc.local_url"/></p>
        </div>
      </t>
    </t>
  </template>
</odoo>
