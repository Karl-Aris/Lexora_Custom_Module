<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_custom_stock_move_line_tree" model="ir.ui.view">
        <field name="name">custom.stock.move.line.tree</field>
        <field name="model">custom.stock.move.line</field>
        <field name="arch" type="xml">
            <tree string="Custom Stock Move Lines">
                <field name="product_id"/>
                <field name="product_uom_qty"/>
                <field name="quantity_done"/>
                <field name="x_custom_field"/>
                <field name="x_custom_note"/>
                <field name="x_computed_field"/>
            </tree>
        </field>
    </record>

    <record id="view_custom_stock_move_line_form" model="ir.ui.view">
        <field name="name">custom.stock.move.line.form</field>
        <field name="model">custom.stock.move.line</field>
        <field name="arch" type="xml">
            <form string="Custom Stock Move Line">
                <sheet>
                    <group>
                        <field name="product_id"/>
                        <field name="product_uom_qty"/>
                        <field name="quantity_done"/>
                        <field name="x_custom_field"/>
                        <field name="x_custom_note"/>
                        <field name="x_computed_field"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
