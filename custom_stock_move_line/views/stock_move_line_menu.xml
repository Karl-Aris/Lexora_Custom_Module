<odoo>
    <!-- Menu -->
    <menuitem id="menu_custom_stock_root" name="Custom Stock" sequence="10"/>
    <menuitem id="menu_x_stock_move_line_custom" name="Custom Stock Move Lines" parent="menu_custom_stock_root" action="action_x_stock_move_line_custom" sequence="20"/>

    <!-- Action -->
    <record id="action_x_stock_move_line_custom" model="ir.actions.act_window">
        <field name="name">Custom Stock Move Lines</field>
        <field name="res_model">x.stock.move.line.custom</field>
        <field name="view_mode">tree,form</field>
    </record>

    <!-- Tree view -->
    <record id="view_x_stock_move_line_custom_tree" model="ir.ui.view">
        <field name="name">x.stock.move.line.custom.tree</field>
        <field name="model">x.stock.move.line.custom</field>
        <field name="arch" type="xml">
            <tree>
                <field name="product_id"/>
                <field name="qty_done"/>
                <field name="x_custom_note"/>
                <field name="x_qty_double"/>
            </tree>
        </field>
    </record>

    <!-- Form view -->
    <record id="view_x_stock_move_line_custom_form" model="ir.ui.view">
        <field name="name">x.stock.move.line.custom.form</field>
        <field name="model">x.stock.move.line.custom</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="product_id"/>
                        <field name="qty_done"/>
                        <field name="x_custom_note"/>
                        <field name="x_qty_double" readonly="1"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
