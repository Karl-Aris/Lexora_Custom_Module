<odoo>
    <template id="sheet_embed_template" name="Google Sheet Page" page="true">
        <t t-call="website.layout">
            <div class="container mt-5 mb-5">
                <h2 class="text-center">Embedded Google Sheet</h2>
                <div class="embed-responsive" style="height: 600px;">
                    <iframe t-att-src="iframe_url"
                            width="100%" height="600" frameborder="0"
                            allowfullscreen></iframe>
                </div>
            </div>
        </t>
    </template>
</odoo>
