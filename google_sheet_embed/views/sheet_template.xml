<odoo>
    <record id="view_form_google_sheet_embed" model="ir.ui.view">
        <field name="name">google.sheet.embed.form</field>
        <field name="model">google.sheet.embed</field>
        <field name="arch" type="xml">
            <form string="Google Sheet Embed">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="iframe_html" invisible="1"/>
                    </group>

                    <!-- This is the key part -->
                    <div type="html" t-if="record.iframe_html.raw_value"
                         t-raw="record.iframe_html.raw_value"/>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
