<odoo>
  <record id="view_google_sheet_embed_form" model="ir.ui.view">
    <field name="name">google.sheet.embed.form</field>
    <field name="model">google.sheet.embed</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="name"/>
            <field name="iframe_html" widget="html_frame"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>

  <record id="view_google_sheet_embed_tree" model="ir.ui.view">
    <field name="name">google.sheet.embed.tree</field>
    <field name="model">google.sheet.embed</field>
    <field name="arch" type="xml">
      <tree>
        <field name="name"/>
      </tree>
    </field>
  </record>

  <record id="action_google_sheet_embed" model="ir.actions.act_window">
    <field name="name">Google Sheets</field>
    <field name="res_model">google.sheet.embed</field>
    <field name="view_mode">tree,form</field>
  </record>

  <menuitem id="menu_google_sheet_root" name="Google Sheet Embed"/>
  <menuitem id="menu_google_sheet_embed" name="Sheets" parent="menu_google_sheet_root" action="action_google_sheet_embed"/>
</odoo>
