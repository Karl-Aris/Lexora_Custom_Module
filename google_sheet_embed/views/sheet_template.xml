<record id="view_x_report_form" model="ir.ui.view">
    <field name="name">x.report.form</field>
    <field name="model">x_report</field>
    <field name="arch" type="xml">
        <form string="Report">
            <sheet>
                <group>
                    <field name="x_name"/>
                    <field name="x_google_url"/>
                </group>
                <group>
                    <field name="x_iframe" widget="html_frame"/>
                </group>
            </sheet>
        </form>
    </field>
</record>
