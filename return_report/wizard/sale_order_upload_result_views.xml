<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
   <record id="sale_order_upload_result_invalid_data_form" model="ir.ui.view">
       <field name="name">sale.order.upload.result.form.view</field>
       <field name="model">sale.order.upload.result</field>
       <field name="arch" type="xml">
         <form>
            <sheet>
                <h2><field name="name" readonly="1"/></h2>
                <group>
                    <field name="attachment" readonly="1" invisible="1"/>
                </group>
                <notebook>
                    <page string="Uploaded Data/s">
                        <field name="order_ids" readonly="1"/>
                    </page>
                    <page string="Invalid Data/s">
                        <field name="invalid_ids" readonly="1">
                            <tree>
                                <field name="name"/>
                                <field name="column"/>
                                <field name="value"/>
                            </tree>
                        </field>
                    </page>
                </notebook>
            </sheet>
         </form>
       </field>
   </record>

    <record id="action_sale_order_upload_result_form" model="ir.actions.act_window">
        <field name="name">Sale Orders Upload Result</field>
        <field name="res_model">sale.order.upload.result</field>
        <field name="view_mode">tree,form</field>
        <field name="view_id" ref="sale_order_upload_result_invalid_data_form"/>
        <field name="context">{'create': False}</field>
        <field name="target">current</field>
    </record>
</odoo>